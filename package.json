{
  "name": "angularts-wp",
  "version": "1.4.0",
  "description": "A starting point for building web applications with AngularJS 1.5 using TypeScript, Webpack and PostCSS",
  "scripts": {
    "build": "npm run constants && webpack --config webpack.config.build.js",
    "constants": "node src/constants",
    "dev": "npm run watch",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --ext=js .",
    "lint:ts": "tslint 'src/**/*.ts' -e src/app/app.constants.ts",
    "postinstall": "npm run typings",
    "shrink": "npm shrinkwrap --dev",
    "start": "node server/server",
    "typings": "typings install",
    "update": "npm install && npm prune --production=false && npm outdated",
    "watch": "parallelshell 'npm run watch:constants' 'webpack-dev-server'",
    "watch:constants": "onchange 'src/constants/**/*.js' -i -- npm run constants"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/angularts-wp.git"
  },
  "keywords": [
    "angularjs",
    "typescript",
    "webpack",
    "typings",
    "postcss"
  ],
  "author": "nicolasmejiaco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nimedev/angularts-wp/issues"
  },
  "homepage": "https://github.com/nimedev/angularts-wp#readme",
  "engines": {
    "npm": ">=3.10.8",
    "node": ">=6.7.0 <7.0.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-cookies": "1.5.8",
    "angular-messages": "1.5.8",
    "angular-translate": "2.12.1",
    "angular-translate-handler-log": "2.12.1",
    "angular-translate-loader-static-files": "2.12.1",
    "angular-translate-storage-cookie": "2.12.1",
    "angular-translate-storage-local": "2.12.1",
    "angular-ui-router": "0.3.1",
    "compression": "1.6.2",
    "express": "4.14.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.0",
    "clean-webpack-plugin": "0.1.10",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "3.6.1",
    "eslint-config-nimedev": "3.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "ip": "1.1.3",
    "json-loader": "0.5.4",
    "niduscss-base": "1.0.1",
    "niduscss-components-button": "1.2.0",
    "niduscss-components-divider": "1.0.0",
    "niduscss-components-dropdown": "1.4.1",
    "niduscss-components-icon-button": "1.0.0",
    "niduscss-elements-form": "1.2.0",
    "niduscss-elements-link": "1.1.1",
    "niduscss-elements-typography": "1.1.2",
    "niduscss-lib-mixins": "1.3.0",
    "niduscss-material-fab": "1.0.0",
    "niduscss-material-icons": "1.0.1",
    "niduscss-utils-align": "1.3.1",
    "niduscss-utils-buttons": "1.0.1",
    "niduscss-utils-color": "1.0.1",
    "niduscss-utils-flex": "1.1.0",
    "niduscss-utils-shadow": "1.3.2",
    "niduscss-utils-state": "1.1.1",
    "niduscss-utils-text": "1.3.2",
    "node-notifier": "4.6.1",
    "onchange": "3.0.2",
    "parallelshell": "2.0.0",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "postcss-mixins": "5.2.0",
    "postcss-nested": "1.0.0",
    "pre-commit": "1.1.3",
    "style-loader": "0.13.1",
    "stylelint": "7.3.1",
    "stylelint-config-nimedev": "7.0.0",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  }
}
