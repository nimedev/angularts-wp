{
  "name": "angularts-wp",
  "version": "4.0.0",
  "description": "A starting point for building web applications with AngularJS 1.5 using TypeScript, Webpack and PostCSS",
  "scripts": {
    "build": "webpack",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "lint:css": "stylelint 'app/**/*.css'",
    "lint:js": "eslint . --ext js",
    "lint:ts": "tslint 'app/**/*.ts' -e 'app/typings/**/*'",
    "postinstall": "npm run typings",
    "start": "npm run watch",
    "typings": "cd app && typings install",
    "watch": "webpack-dev-server"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/angularts-wp.git"
  },
  "keywords": [
    "angularjs",
    "typescript",
    "webpack",
    "typings",
    "postcss"
  ],
  "author": "nicolasmejiaco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nimedev/angularts-wp/issues"
  },
  "homepage": "https://github.com/nimedev/angularts-wp#readme",
  "engines": {
    "npm": ">=3.10.9",
    "node": ">=6.9.2 <7.0.0"
  },
  "dependencies": {
    "angular": "1.6.1",
    "niduscss-base": "1.0.1",
    "niduscss-elements-form": "1.3.0",
    "niduscss-elements-link": "1.1.1",
    "niduscss-elements-typography": "1.1.2",
    "niduscss-lib-mixins": "1.4.0",
    "niduscss-utils-align": "1.3.1",
    "niduscss-utils-buttons": "1.0.1",
    "niduscss-utils-color": "1.0.1",
    "niduscss-utils-flex": "1.1.0",
    "niduscss-utils-shadow": "1.3.2",
    "niduscss-utils-state": "1.1.1",
    "niduscss-utils-text": "1.3.2"
  },
  "devDependencies": {
    "autoprefixer": "6.6.0",
    "clean-webpack-plugin": "0.1.14",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "eslint": "3.12.2",
    "eslint-config-nimedev-base": "1.0.0",
    "eslint-plugin-import": "2.2.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "ip": "1.1.4",
    "json-loader": "0.5.4",
    "opener": "1.4.2",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-import": "9.0.0",
    "postcss-loader": "1.2.1",
    "postcss-mixins": "5.4.1",
    "postcss-nested": "1.0.0",
    "pre-commit": "1.2.2",
    "style-loader": "0.13.1",
    "stylelint": "7.7.0",
    "stylelint-config-nimedev": "7.0.0",
    "ts-loader": "1.3.3",
    "tslint": "4.2.0",
    "tslint-loader": "3.3.0",
    "typescript": "2.1.4",
    "typings": "2.1.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "2.0.0",
    "webpack-validator": "2.3.0"
  }
}
