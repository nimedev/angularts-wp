{
  "name": "angularts-wp",
  "version": "2.1.1",
  "description": "A starting point for building web applications with AngularJS 1.5 using TypeScript, Webpack and PostCSS",
  "scripts": {
    "build": "npm run constants && webpack",
    "constants": "node config",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --ext=js .",
    "lint:ts": "tslint 'src/**/*.ts' -e 'src/constants/constants.ts' -e 'src/typings/**/*'",
    "open": "node libs/opener",
    "postinstall": "npm run typings",
    "start": "npm run open && npm run watch",
    "typings": "cd src && typings install",
    "watch": "parallelshell 'npm run watch:constants' 'webpack-dev-server'",
    "watch:constants": "onchange 'config/**/*.js' -i -- npm run constants"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/angularts-wp.git"
  },
  "keywords": [
    "angularjs",
    "typescript",
    "webpack",
    "typings",
    "postcss"
  ],
  "author": "nicolasmejiaco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nimedev/angularts-wp/issues"
  },
  "homepage": "https://github.com/nimedev/angularts-wp#readme",
  "engines": {
    "npm": ">=3.10.8",
    "node": ">=6.8.1 <7.0.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "niduscss-base": "1.0.1",
    "niduscss-elements-form": "1.2.0",
    "niduscss-elements-link": "1.1.1",
    "niduscss-elements-typography": "1.1.2",
    "niduscss-lib-mixins": "1.3.0",
    "niduscss-utils-align": "1.3.1",
    "niduscss-utils-buttons": "1.0.1",
    "niduscss-utils-color": "1.0.1",
    "niduscss-utils-flex": "1.1.0",
    "niduscss-utils-shadow": "1.3.2",
    "niduscss-utils-state": "1.1.1",
    "niduscss-utils-text": "1.3.2"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "clean-webpack-plugin": "0.1.13",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-config-nimedev": "3.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "ip": "1.1.3",
    "json-loader": "0.5.4",
    "onchange": "3.0.2",
    "opener": "1.4.2",
    "parallelshell": "2.0.0",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-import": "8.1.2",
    "postcss-loader": "1.0.0",
    "postcss-mixins": "5.4.0",
    "postcss-nested": "1.0.0",
    "pre-commit": "1.1.3",
    "style-loader": "0.13.1",
    "stylelint": "7.4.2",
    "stylelint-config-nimedev": "7.0.0",
    "ts-loader": "0.9.4",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.15.0",
    "webpack-validator": "2.2.9"
  }
}
